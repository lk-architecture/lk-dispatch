  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.13",
    "babel-core": "^6.3.13",
    "babel-eslint": "^5.0.0-beta4",
    "babel-plugin-rewire": "^1.0.0-beta-2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "codecov": "^1.0.1",
    "eslint": "^1.10.3",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "rimraf": "^2.5.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "validator": "^4.4.0"
  },
